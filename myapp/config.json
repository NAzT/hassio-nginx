{
  "name": "My Custom Hass.io Nginx",
  "version": "1.0.4",
  "slug": "custom-hassio-nginx",
  "description": "A custom hassio nginx",
  "url": "https://github.com/NAzT/hassio-nginx",
  "webui": "http://[HOST]:[PORT:9999]",
  "map": [
    "ssl:ro",
    "config:rw"
  ],
  "arch": [
    "aarch64",
    "amd64",
    "armhf",
    "armv7",
    "i386"
  ],
  "host_network": true,
  "startup": "system",
  "boot": "auto",
  "options": {
    "items": [{
      "path": "/hello",
      "url": "http://localhost:8111"
    }, {
      "path": "/1880",
      "url": "http://localhost:1880"
    }]
  },
  "schema": {
    "items": [{
      "path": "str",
      "url": "url"
    }]
  }
}
